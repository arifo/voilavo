getUserFromDB(ref, child) {
  return firebase
    .database()
    .ref(ref)
    .child(child)
    .once('value', snapshot => snapshot.val());
}

updateDB(fullPath, params) {
  return firebase
    .database()
    .ref(fullPath)
    .update(params);
}






